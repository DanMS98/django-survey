name: \"Question\",\n  props: {\n    question: Object,\n  },\n  data() {\n    return {};\n  },\n  methods: {\n    onNext() {\n      this.$emit(\"goNext\", this.question.id);\n    },\n    onPrev() {\n      this.$emit(\"goPrev\", this.question.id);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.question {\n  box-shadow: 0 8px 10px 0 rgba(0, 0, 0, 0.2);\n  border: 2px solid rgb(30, 32, 32);\n  border-left: 10px solid rgb(218, 11, 11);\n  border-bottom: 10px solid rgb(218, 11, 11);\n  margin: 10px;\n  padding: 70px 20px;\n}\n.question.answered {\n  border-left: 10px solid rgb(9, 134, 30);\n  border-bottom: 10px solid rgb(9, 134, 30);\n}\n\n.wrap {\n  font: 12px Arial, san-serif;\n}\nh1.likert-header {\n  padding-left: 4.25%;\n  margin: 20px 0 0;\n}\ndiv .statement {\n  display: block;\n  font-size: 14px;\n  font-weight: bold;\n  padding: 30px 0 0 4.25%;\n  margin-bottom: 10px;\n}\ndiv .likert {\n  list-style: none;\n  width: 100%;\n  margin: 0;\n  padding: 0 0 35px;\n  display: block;\n  border-bottom: 2px solid #efefef;\n}\ndiv .likert:last-of-type {\n  border-bottom: 0;\n}\ndiv .likert:before {\n  content: \"\";\n  position: relative;\n  top: 11px;\n  left: 9.5%;\n  display: block;\n  background-color: #efefef;\n  height: 4px;\n  width: 78%;\n}\ndiv .likert li {\n  display: inline-block;\n  width: 19%;\n  text-align: center;\n  vertical-align: top;\n}\ndiv .likert li input[type=\"radio\"] {\n  display: block;\n  position: relative;\n  top: 0;\n  left: 50%;\n  margin-left: -6px;\n}\ndiv .likert li label {\n  width: 100%;\n}\ndiv .buttons {\n  margin: 30px 0;\n  padding: 0 4.25%;\n  text-align: right;\n}\ndiv .buttons button {\n  padding: 5px 10px;\n  background-color: #67ab49;\n  border: 0;\n  border-radius: 3px;\n}\ndiv .buttons .previous {\n  background-color: #e9e9e9;\n}\ndiv .buttons .next {\n  background-color: #67ab49;\n}\ndiv .buttons .clear:hover {\n  background-color: #ccc;\n}\nform .buttons .submit:hover {\n  background-color: #14892c;\n}\n</style>"],"sourc