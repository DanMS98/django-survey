le.log(\"choices are JSON: \", JSON.stringify(this.parentChoices));\n        console.log(\"choices are STRING:\", finalString);\n      } else {\n        this.survey[id].isAnswered = true;\n        this.survey[id].isSelected = false;\n        this.survey[id + 1].isSelected = true;\n      }\n    },\n    showPrevQuestion(id) {\n      if (id != 0) {\n        console.log(id);\n        this.survey[id].isSelected = false;\n        this.survey[id - 1].isSelected = true;\n      } else {\n        alert(\"سوال قبلی ای وجود ندارد\");\n      }\n    },\n  },\n\n  data() {\n    return {\n      survey: [],\n      parentChoices: [],\n      childAge: \"\",\n      childGender:\"\",\n    };\n  },\n\n  created() {\n    this.survey = [\n      {\n        id: 0,\n        title: \"خودآگاهی هیجانی\",\n        value: \"Emotional_self-awareness1\",\n        text: \"بیشتر اوقات فرزندم متوجه نمیشود چه احساسی دارد‬\",\n        isAnswered: false,\n        choice: \"\",\n        isSelected: true,\n      },\n      {\n        id: 1,\n        title: \"خودآگاهی هیجانی\",\n        value: \"Emotional_self-awareness2\",\n        text: \"وقتی یکی از دوستانش ناراحت باشد او متوجه نمیشود.\",\n        isAnswered: false,\n        choice: \"\",\n        isSelected: false,\n      },\n      // {\n      //   id: 2,\n      //   title: \"خودآگاهی هیجانی\",\n      //   value: \"Emotional_self-awareness3\",\n      //   text: \"دقيقا مى داند چه احساسى دارد.\",\n      //   isAnswered: false,\n      //   choice: \"\",\n      //   isSelected: false,\n      // },\n      // {\n      //   id: 3,\n      //   title: \"خودآگاهی هیجانی\",\n      //   value: \"Emotional_self-awareness4\",\n      //   text: \"احساسات دیگران را میفهمد\",\n      //   isAnswered: false,\n      //   choice: \"\",\n      //   isSelected: false,\n      // },\n      // {\n      //   id: 4,\n      //   title: \"خودآگاهی هیجانی\",\n      //   value: \"Emotional_self-awareness5\",\n      //   text: \"درك احساسات ديگران برايش دشوار است.\",\n      //   isAnswe